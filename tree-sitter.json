    "license": "MIT",
    "description": "Garnet grammar for tree-sitter",
    "authors": [
      {
        "name": "ark231",
        "url": "https://ark231.github.io/"
      }
    ],
    "links": {
      "repository": "https://github.com/ark231/Garnet"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
